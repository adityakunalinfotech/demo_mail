<h1>Listing my_mails</h1>

<table>
  <tr>
    <th>Mailm</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @my_mails.each do |my_mail| %>
  <tr>
    <td><%= my_mail.mailm %></td>
    <td><%= link_to 'Show', my_mail %></td>
    <td><%= link_to 'Edit', edit_my_mail_path(my_mail) %></td>
    <td><%= link_to 'Destroy', my_mail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New My mail', new_my_mail_path %>
